    "embed-wasm": "node embed-wasm.js $SIMD $SCALAR"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eshaz/synaudio.git"
  },
  "author": {
    "name": "Ethan Halsall",
    "email": "ethanhalsall@pm.me"
  },
  "license": "LGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/eshaz/synaudio/issues"
  },
  "type": "module",
  "homepage": "https://github.com/eshaz/synaudio#readme",
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "jest": "^27.5.1",
    "mpg123-decoder": "^0.3.14",
    "opus-decoder": "^0.4.6",
    "prettier": "^2.7.1"
  },
  "dependencies": {
    "simple-yenc": "^0.2.1",
    "web-worker": "^1.2.0"
  }
