{
  "name": "synaudio",
  "version": "0.0.1",
  "description": "Library that finds the synchronization point between two similar audio clips.",
  "keywords": [
    "wasm",
    "simd",
    "web worker",
    "correlation",
    "audio",
    "synchronization"
  ],
  "main": "index.js",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/.bin/jest --maxWorkers=100%",
